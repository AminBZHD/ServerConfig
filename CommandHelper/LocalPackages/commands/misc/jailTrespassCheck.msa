jailtrespasscheck:/jailtrespasscheck $x $y $z $radius $world = >>>
    
	@location = array(x: $x, y: $y, z: $z, world: $world);
	@theshold = @location[y] + 1;
	@radius = $radius
	
	set_timeout(2000, closure(
	
		try {
		
			@players = players_in_radius(@location, @radius);
			
			foreach(@player in @players){
			
				if(!ploc(@player)[y] > @threshold){
				
					sudo('/arrest '.@player.' Arrested by self for jumping into jail');
				
				}
			
			}
		
		} catch (NotFoundException @ErrorInfo) {
		
			msg('No players found');
			die();
		
		}		
		
	))
<<<
